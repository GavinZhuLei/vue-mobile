{
  "presets": [
    ["env", {
      "modules": false
    }],
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    "transform-vue-jsx",

    ["component",
      {
        "libraryName": "mint-ui",
        "style": true
      }
    ],
    ["import", {
      "libraryName": "mand-mobile",
      "libraryDirectory": "components"
    }]
  ]
}
